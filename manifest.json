{
    "name": "Extension Scaffold",
    "version": "1.0",
    "description": "A study in Chrome extensions",
    "manifest_version": 2,

    "incognito": "split",

    "browser_action": {
        "default_popup": "html/popup.html"
    },

    "permissions": [
        "activeTab",
        "contextMenus"
    ],

    "icons": {
        "128": "icon.png"
    }
}
